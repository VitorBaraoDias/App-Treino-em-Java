<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.treino_casa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconmuscle"
        android:supportsRtl="true"
        android:theme="@style/Theme.Treino_casa">
        <activity
            android:name=".ActivityProfile"
            android:exported="false" />
        <activity
            android:name=".FormListaPersonalTrainer"
            android:exported="false" />
        <activity
            android:name=".FormTrainer"
            android:exported="false" />
        <activity
            android:name=".ActivityMenu"
            android:exported="true"

            android:screenOrientation="portrait" />
        <activity
            android:name=".Caracteristicas"
            android:exported="true"
            android:screenOrientation="portrait" />
